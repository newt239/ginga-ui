    "./package.json": "./package.json",
    "./index.css": "./dist/index.css",
    "./variables.css": "./dist/variables.css"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "homepage": "https://github.com/newt239/ginga-ui#readme"
